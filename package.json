{
  "name": "@jedwards1211/codemods",
  "version": "0.0.0-development",
  "description": "my personal jscodeshift codemods",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "lint": "eslint src test --cache",
    "lint:fix": "eslint --fix src test --cache",
    "lint:watch": "esw --watch src test --cache",
    "prettier": "prettier --write *.json *.md *.js src/**/*.js test/**/*.js",
    "prettier:check": "prettier --list-different *.json *.md *.js src/**/*.js test/**/*.js",
    "flow": "flow",
    "flow:coverage": "for file in src/**.js test/**.js; do echo $file; flow coverage $file; done",
    "flow:watch": "flow-watch -e js,js.flow,flowconfig --ignore es/ --ignore node_modules/ --watch .flowconfig --watch src/ --watch test/",
    "clean": "rimraf es lib $(cd src; ls) *.js.flow",
    "build": "npm run clean && babel src --out-dir es && flow-copy-source -v src/ es && cross-env BABEL_ENV=es5 babel src --out-dir . && flow-copy-source -v src/ .",
    "test": "cross-env NODE_ENV=test BABEL_ENV=es5 mocha $npm_package_config_mocha && cross-env NODE_ENV=test BABEL_ENV=coverage nyc --reporter=lcov --reporter=text mocha $npm_package_config_mocha",
    "test:watch": "cross-env NODE_ENV=test BABEL_ENV=test mocha --watch $npm_package_config_mocha",
    "test:debug": "cross-env NODE_ENV=test BABEL_ENV=test mocha --inspect-brk $npm_package_config_mocha",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov; codecov",
    "prepublishOnly": "npm run clean && npm run prettier:check && npm run lint && flow && npm test && npm run build",
    "open:coverage": "open coverage/lcov-report/index.html",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint && flow",
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@jedwards1211/commitlint-config"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "config": {
    "mocha": "-r @babel/register test/configure.js test/**/*.js test/*.js",
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jedwards1211/codemods.git"
  },
  "keywords": [
    "jscodeshift",
    "codemod",
    "refactoring",
    "react",
    "sequelize"
  ],
  "author": "Andy Edwards",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jedwards1211/codemods/issues"
  },
  "homepage": "https://github.com/jedwards1211/codemods#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@commitlint/cli": "^6.0.2",
    "@commitlint/config-conventional": "^6.0.2",
    "@jedwards1211/commitlint-config": "^1.0.0",
    "@jedwards1211/eslint-config": "^2.0.0",
    "@jedwards1211/eslint-config-flow": "^2.0.0",
    "@material-ui/system": "^4.5.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "chai": "^4.2.0",
    "codecov": "^3.1.0",
    "copy": "^0.3.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-watch": "^4.0.2",
    "flow-bin": "^0.92.0",
    "flow-copy-source": "^2.0.2",
    "flow-watch": "^1.1.4",
    "husky": "^1.1.4",
    "istanbul": "^0.4.5",
    "lint-staged": "^8.0.4",
    "mocha": "^6.2.1",
    "nyc": "^13.1.0",
    "prettier": "^1.15.2",
    "prettier-eslint": "^8.8.2",
    "rimraf": "^2.6.0",
    "semantic-release": "^15.1.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.1.5",
    "atom-space-pen-views": "^2.2.0",
    "babel-types": "^6.26.0",
    "babylon": "^6.18.0",
    "find-root": "^1.1.0",
    "fs-extra": "^6.0.1",
    "glob-gitignore": "^1.0.11",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "inflection": "^1.12.0",
    "jscodeshift": "0.6.4",
    "jscodeshift-add-imports": "^1.0.0",
    "jscodeshift-find-imports": "^1.0.1",
    "json5": "^1.0.1",
    "lodash": "^4.17.11",
    "lodash-es": "^4.17.10",
    "lodash.map": "^4.6.0",
    "lodash.upperfirst": "^4.3.1",
    "promisify-child-process": "^1.0.6",
    "recast": "0.16.2",
    "require-glob": "^3.2.0",
    "reselect": "^3.0.1",
    "superagent": "^3.8.3"
  },
  "renovate": {
    "extends": [
      ":separateMajorReleases",
      ":combinePatchMinorReleases",
      ":ignoreUnstable",
      ":prImmediately",
      ":renovatePrefix",
      ":updateNotScheduled",
      ":preserveSemverRanges",
      ":semanticPrefixFixDepsChoreOthers",
      ":automergeDisabled",
      "group:monorepos"
    ],
    "automerge": true,
    "major": {
      "automerge": false
    }
  }
}
